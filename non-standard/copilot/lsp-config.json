{
  "lspServers": {
    "typescript": {
      "command": "typescript-language-server",
      "args": ["--stdio"],
      "fileExtensions": {
        ".ts": "typescript",
        ".tsx": "typescript",
        ".js": "javascript"
      }
    },
    "vue": {
      "command": "vue-language-server",
      "args": ["--stdio"],
      "fileExtensions": {
        ".vue": "vue"
      }
    }
  }
}
